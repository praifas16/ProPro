    "security-check": "npm audit", 
    "security-fix": "npm audit fix" 
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@eslint/js": "^9.11.1",
    "@testing-library/cypress": "^10.0.2",
    "babel-jest": "^29.7.0",
    "cypress": "^13.14.2",
    "eslint": "^8.57.1",
    "jest": "^29.7.0"
  },
  "dependencies": {
    "firebase": "^8.10.1",
    "globals": "^15.9.0",
    "text-encoding": "^0.7.0"
  }
}
