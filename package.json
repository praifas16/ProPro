{
  "name": "project-comparison",
  "version": "1.0.0",
  "scripts": {
    "start": "node index.js",
    "build": "echo 'Building project...' && mkdir build && echo '<html><body><h1>Build Completed</h1></body></html>' > build/index.html",
    "lint": "eslint .",
    "test": "jest --coverage",
    "integration-test": "echo 'Running integration tests...' && jest --config jest.integration.config.js",
    "e2e": "cypress open",
    "audit": "npm audit",
    "deploy": "echo 'Deploying project...' && cp -r build/* /path/to/deployment/directory"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "babel-jest": "^29.7.0",
    "codecov": "^3.8.3",
    "cypress": "^13.14.2",
    "eslint": "^8.0.0",
    "eslint-plugin-html": "^8.1.1",
    "firebase": "^8.10.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^25.0.1"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "dependencies": {
    "text-encoding": "^0.7.0"
  }
}
